{{include "partials/head" site page}}
<body>
	<div class="container">
		<div class="left-col">
			<header id="header" class="inner">{{include "partials/header" site page}}</header>
		</div>
		<div class="mid-col">
			<div class="mid-col-container">
				<div class="list-header">
					<div class="title">{{page.TermsCount}} {{page.PageType}}</div>
				</div>
				<div id="content" class="inner">
					<div itemscope itemtype="http://schema.org/Blog">
{{for term in page.Terms}}

						<div class="list-entry {{page.PageType}}" itemprop="name"><a href="{{site.BaseURL}}{{page.TermsType}}/{{string_urlize term}}/">{{term}}</a></div>
{{end}}
					</div>
				</div>
			</div>
			<footer id="footer" class="inner">{{include "partials/footer" site page}}</footer>
		</div>
	</div>
</body>
</html>
