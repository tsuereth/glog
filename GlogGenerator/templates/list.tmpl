			<header id="header" class="inner">
				{{include "partials/header" ~}}
			</header>
		</div>
		<div class="mid-col">
			<div class="mid-col-container">
				<div class="list-header">
					<div class="title">{{page.linked_posts_count}} posts in <span class="{{page.page_type}}">{{page.title}}</span></div>
